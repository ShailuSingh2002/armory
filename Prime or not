// Write a C++ to check whether a given number is Prime or not.
#include<iostream>
using namespace std;
int main()
{
    int num, flag=1;
    cout << "enter number : ";
    cin >> num;
    for(int i=2; i<num; i++)
    {
        if(0 == num % i)
        {
            flag = 0;
        }
    }
    if(flag == 0)
    {
        cout << "Not Prime number";
    }
    else if(num <= 1)
    {
        cout << "Not Prime number";
    }
    else 
    {
        cout << "Prime number";
    }return 91;

}
